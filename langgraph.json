{
  "dependencies": ["."],
  "graphs": {
    "grammarly_support": {
      "path": "src/app.py:agent",
      "entrypoint": "agent",
      "name": "Grammarly Support Chatbot (JSON)",
      "description": "Full graph with detailed state - requires JSON input",
      "env": {
        "TENSORZERO_GATEWAY_URL": "http://localhost:3000",
        "PYTHONPATH": "."
      }
    },
    "grammarly_chat": {
      "path": "src/simple_chat.py:simple_chat",
      "entrypoint": "simple_chat",
      "name": "Grammarly Support Chat",
      "description": "Simple chat interface for demos",
      "env": {
        "TENSORZERO_GATEWAY_URL": "http://localhost:3000",
        "PYTHONPATH": "."
      }
    }
  },
  "env": {
    "LANGCHAIN_TRACING_V2": "true",
    "LANGCHAIN_PROJECT": "grammarly-support-chatbot"
  },
  "cli": {
    "version": "0.1.0"
  }
}